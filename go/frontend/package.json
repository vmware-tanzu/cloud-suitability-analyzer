{
  "name": "cloud-suitability-analyzer",
  "version": "2.1.0",
  "scripts": {
    "ng": "ng",
    "dev-start-frontend-backend": "concurrently \"pushd .. && go generate && popd && go run rules-admin.go --database-dir=./data\" \"ng serve --proxy-config proxy-conf.json\"",
    "dev-start-frontend": "ng serve --proxy-config proxy-conf.json",
    "dev-start-backend": "go run rules-admin.go --database-dir=../exe",
    "production-build": "ng build --prod > /dev/null 2>&1",
    "source-map": "cd dist/ng-app && source-map-explorer *.js",
    "build": "ng build",
    "build-release": "ng build --configuration=production",
    "test": "ng test --codeCoverage=true --source-map=false",
    "lint": "ng lint",
    "e2e": "concurrently \"pushd .. && go generate && popd && go run rules-admin.go --database-dir=./data\" \"ng e2e\"",
    "webdriver-manager": "webdriver-manager update"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@cds/city": "^1.1.0",
    "@cds/core": "^5.3.0",
    "@cds/angular": "^5.3.0",
    "@clr/angular": "5.3.0",
    "@clr/icons": "^5.3.0",
    "@clr/ui": "^5.3.0",
    "@angular/cdk": "^9.2.4",
    "@swimlane/ngx-charts": "^13.0.4",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "d3": "6.7.0",
    "lodash": "4.17.21",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.4",
    "zone.js": "~0.11.3",
    "ngx-toastr": "^14.0.0",
    "webdriver-manager": "^12.1.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "6.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-jasmine-seed-reporter": "^0.2.0",
    "karma-junit-reporter": "~2.0.1",
    "karma-spec-reporter": "0.0.32",
    "postcss-cli": "7.1.0",
    "protractor": "~7.0.0",
    "sass": "1.32.8",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
